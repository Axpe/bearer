<h1>Nuevo Contacto</h1>

<%= form_for :contact, url: contacts_path do |f| %>
  <p>
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </p>

  <p>
    <%= f.label :shippingcity %><br>
    <%= f.text_area :shippingcity %>
  </p>
  
  <p>
    <%= f.label 'Cuenta' %><br>
    <%= select(:contact, :account__bearerid__c, Account.all.collect { |c| [c.name,c.id] }) %>
  </p>

  <p><%= f.submit %></p>
<% end %>